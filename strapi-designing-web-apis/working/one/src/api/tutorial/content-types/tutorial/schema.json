{
  "kind": "collectionType",
  "collectionName": "tutorials",
  "info": {
    "singularName": "tutorial",
    "pluralName": "tutorials",
    "displayName": "Tutorial",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "minLength": 4,
      "maxLength": 120,
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "video",
        "text",
        "podcast"
      ],
      "default": "video"
    },
    "url": {
      "type": "uid"
    },
    "classroom": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::classroom.classroom",
      "inversedBy": "tutorials"
    },
    "summary": {
      "type": "text",
      "maxLength": 200
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "text",
      "required": true
    }
  }
}
