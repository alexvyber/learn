---
export async function getStaticPaths() {
  const response = await fetch("https://jsonplaceholder.typicode.com/posts")
  const posts = (await response.json()) as any[]

  return posts.map(post => ({
    params: { id: post.id },
    props: { post },
  }))
}

const { post } = Astro.props
---

<>
  <h1>{post.title}</h1>
  {post.body}
</>
